{% block html_head_container %}

	{% include 'partial/html-header.twig' %}

	{% block head %}
	{% endblock %}

	</head>
{% endblock %}

	<body class="{{body_class}}" data-template="base.twig">

		{{ function('wp_body_open') }}

		<a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content') }}</a>

		<header class="header" >
			{% block header %}

				<div class="wrapper">
					<span class="hdr-logo" role="banner">
						<a class="hdr-logo-link" href="{{site.url}}" rel="home">{{site.name}}</a>
					</span>
					<nav id="nav-main" class="nav-main" role="navigation">
						{% include "partial/menu.twig" with {'items': menu.get_items} %}
					</nav><!-- #nav -->
				</div>

			{% endblock %}
		</header>

		<section id="content" role="main" class="content-wrapper">
			{% if title %}<h1>{{title}}</h1>{% endif %}
			<div class="wrapper">
				{% block content %}
					Sorry, no content
				{% endblock %}
			</div>

		</section>

		{% block footer %}

				{% include 'partial/footer.twig' %}

		{% endblock %}

		{{ function('wp_footer') }}

	</body>
</html>
